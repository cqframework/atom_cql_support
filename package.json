{
  "name": "language-cql",
  "version": "2.9.1",
  "description": "Clinical Quality Language support in Atom",
  "homepage": "https://github.com/cqframework/atom_cql_support#readme",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/cqframework/atom_cql_support.git"
  },
  "bugs": {
    "url": "https://github.com/cqframework/atom_cql_support/issues"
  },
  "dependencies": {
    "atom-languageclient": "^0.9.9",
    "atom-package-deps": "^5.1.0",
    "child_process": "^1.0.2",
    "file-url": "^3.0.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "nuclide-commons-atom": "^0.7.2",
    "tmp": "0.2.1"
  },
  "package-deps": [
    "atom-ide-ui"
  ],
  "devDependencies": {
    "@types/atom": "^1.26.0",
    "@types/classnames": "^2.2.11",
    "@types/dompurify": "^2.2.1",
    "@types/jasmine": "^3.6.4",
    "@types/node": "^14.14.34",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "apm": "^1.0.2",
    "yarn": "^1.22.10"
  },
  "private": true,
  "engines": {
    "atom": ">= 1.25.0",
    "node": ">= 10"
  },
  "main": "./main",
  "workspaces": [
    "language-cql/pkg/*"
  ],
  "configSchema": {
    "cqlVersion": {
      "order": 10,
      "title": "CQL Language Version",
      "type": "string",
      "enum": [
        "1.5"
      ],
      "default": "1.5"
    },
    "javaHome": {
      "order": 20,
      "title": "Java Home",
      "type": "string",
      "default": "",
      "description": "Absolute path to Java 8 or later home folder used to launch the CQL language server (overrides JAVA_HOME and JDK_HOME)."
    }
  }
}
