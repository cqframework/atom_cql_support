      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  },
  "languageServer": {
    "groupId": "info.cqframework",
    "artifactId": "cql.ls",
    "version": "0.1-SNAPSHOT",
    "classifier": "shaded"
  },
  "cqlService": {
    "groupId": "com.alphora.cql",
    "artifactId": "cli",
    "version": "1.0-SNAPSHOT",
    "classifier": "shaded"
  },
  "configSchema": {
    "cqlVersion": {
      "order": 10,
      "title": "CQL Language Version",
      "type": "string",
      "enum": [
        "1.3"
      ],
      "default": "1.3"
    },
    "terminologyServer": {
      "order": 20,
      "title": "Terminology Server",
      "type": "string",
      "default": "",
      "description": "Url for external terminology server if desired."
    },
    "toolDebug": {
      "order": 30,
      "title": "Java Components Debug",
      "type": "boolean",
      "default": false,
      "description": "Start the language server and cli with Java debug options enabled. WARNING: Developer use only."
    }
  },
