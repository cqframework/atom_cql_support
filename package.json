{
  "name": "language-cql",
  "main": "./lib/main",
  "version": "2.0.4-beta",
  "description": "Clinical Quality Language support in Atom",
  "repository": "https://github.com/cqframework/atom_cql_support",
  "license": "Apache 2.0",
  "dependencies": {
    "apm": "^1.0.2",
    "atom-languageclient": "^0.8.3",
    "atom-package-deps": "^5.1.0",
    "child_process": "^1.0.2",
    "path": "^0.12.7"
  },
  "package-deps": [
    "atom-ide-ui"
  ],
  "engines": {
    "atom": "*",
    "node": "*"
  },
  "enhancedScopes": [
    "source.rkt"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  },
  "providedServices": {
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  }
}
