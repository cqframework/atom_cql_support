{
  "name": "language-cql-client",
  "version": "0.0.0",
  "description": "Clinical Quality Language Server Client",
  "main": "./lib/main",
  "package-deps": [
    "atom-ide-ui"
  ],
  "dependencies": {
    "language-cql-common" : "0.0.0"
  },
  "javaDependencies": {
    "cql-language-server": {
      "groupId": "org.opencds.cqf.cql.ls",
      "artifactId": "cql-ls",
      "version": "1.5.2-SNAPSHOT",
      "classifier": "shaded"
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "cql-language-client": {
      "versions": {
        "0.1.0": "provideLanguageClient"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  },
  "configSchema": {
    "cqlVersion": {
      "order": 10,
      "title": "CQL Language Version",
      "type": "string",
      "enum": [
        "1.5"
      ],
      "default": "1.5"
    }
  }
}