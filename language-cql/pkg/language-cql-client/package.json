{
  "name": "language-cql-client",
  "version": "0.0.0",
  "description": "Clinical Quality Language Server Client",
  "main": "./lib/main",
  "dependencies": {
    "atom-languageclient": "^0.9.9",
    "atom-package-deps": "^5.1.0",
    "child_process": "^1.0.2",
    "file-url": "^3.0.0",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9",
    "language-cql-common" : "0.0.0"
  },
  "package-deps": [
    "atom-ide-ui"
  ],
  "javaDependencies": {
    "cql-language-server": {
      "groupId": "org.opencds.cqf.cql",
      "artifactId": "ls",
      "version": "1.5.0-SNAPSHOT",
      "classifier": "shaded"
    }
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "console": {
      "versions": {
        "0.1.0": "consumeConsole"
      }
    },
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    },
    "atom-ide-busy-signal": {
      "versions": {
        "0.1.0": "consumeBusySignal"
      }
    }
  },
  "providedServices": {
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    }
  },
  "configSchema": {
    "cqlVersion": {
      "order": 10,
      "title": "CQL Language Version",
      "type": "string",
      "enum": [
        "1.5"
      ],
      "default": "1.5"
    }
  }
}